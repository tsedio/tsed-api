{
  "name": "@tsed/server",
  "version": "1.11.0",
  "description": "Server based on Ts.ED framework",
  "author": "Romain Lenzotti",
  "private": true,
  "scripts": {
    "build": "yarn tsc",
    "tsc": "tsc --project tsconfig.compile.json",
    "tsc:w": "tsc --project tsconfig.json -w",
    "start": "tsnd --inspect --ignore-watch node_modules --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "test": "yarn test:lint && yarn test:coverage",
    "test:unit": "cross-env NODE_ENV=test CI=1 jest --detectOpenHandles --coverage",
    "test:coverage": "yarn test:unit",
    "test:lint": "yarn eslint src --ext .ts",
    "test:lint:fix": "yarn eslint src --ext .ts --fix",
    "prettier:fix": "yarn prettier \"{src,test}/**/*.ts\" --write",
    "ngrok": "ngrok http 8083"
  },
  "dependencies": {
    "@octokit/rest": "18.3.2",
    "@tsed/ajv": "6.52.0",
    "@tsed/async-hook-context": "6.52.0",
    "@tsed/common": "6.52.0",
    "@tsed/core": "6.52.0",
    "@tsed/di": "6.52.0",
    "@tsed/exceptions": "6.52.0",
    "@tsed/formio": "6.52.0",
    "@tsed/json-mapper": "6.52.0",
    "@tsed/logger-logentries": "5.15.0",
    "@tsed/mongoose": "6.52.0",
    "@tsed/passport": "6.52.0",
    "@tsed/platform-express": "6.52.0",
    "@tsed/schema": "6.52.0",
    "@tsed/swagger": "6.52.0",
    "ajv": "8.5.0",
    "body-parser": "^1.19.0",
    "cache-manager": "3.4.1",
    "cache-manager-redis-store": "2.0.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "8.2.0",
    "express": "^4.17.1",
    "formio": "2.0.0",
    "globby": "11.0.2",
    "le_node": "^1.8.0",
    "method-override": "^3.0.0",
    "moment": "2.29.1",
    "mongoose": "^5.12.2",
    "passport": "^0.4.1",
    "passport-github": "^1.1.0",
    "url-parse": "1.4.7"
  },
  "devDependencies": {
    "@tsed/cli-plugin-eslint": "3.1.0",
    "@tsed/cli-plugin-jest": "3.1.0",
    "@tsed/cli-plugin-mongoose": "3.1.0",
    "@tsed/cli-plugin-passport": "3.1.0",
    "@tsed/testing-mongoose": "6.52.0",
    "@types/cache-manager": "3.4.0",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.9",
    "@types/dotenv": "8.2.0",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/method-override": "^0.0.31",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.14.22",
    "@types/passport": "^1.0.5",
    "@types/passport-github": "1.1.5",
    "@types/supertest": "^2.0.10",
    "@types/url-parse": "^1.4.3",
    "concurrently": "^5.3.0",
    "husky": "^5.0.9",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "ngrok": "3.4.1",
    "nodemon": "^2.0.7",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.0",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "3.9.0",
    "typescript": "^4.2.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  },
  "resolutions": {
    "mongoose": "5.12.2"
  },
  "tsed": {
    "packageManager": "yarn",
    "convention": "std",
    "paths": {
      "middlewares": "<rootDir>/src/infra/middlewares",
      "services": "<rootDir>/src/back"
    }
  },
  "peerDependencies": {}
}